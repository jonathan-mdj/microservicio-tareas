{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/gui/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.ico)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.png)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.svg)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.woff2)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "font/woff2"
      }
    },
    {
      "src": "/(.*\\.woff)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "font/woff"
      }
    },
    {
      "src": "/(.*\\.ttf)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "font/ttf"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_URL": "https://microservicio-backend.onrender.com"
  }
}
